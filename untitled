


public String[] makeUnique(String[] list){
	
	ArrayList<String> unique = new ArrayList<String>();
	for(int x=0;x<list.length;x++){
		boolean contains = false;
		for(int y=0;y<unique.size();y++){
			if(unique.get(y).equals(list[x])){
				contains = true;
				break;
			}
		}
		if(!contains){
			unique.add(list[x]);
		}
	}
	String[] toReturn = new String[unique.size()];
	for(int x=0;x<toReturn.length;x++){
		toReturn[x] = unique.get(x);
	}
	return toReturn;
} 